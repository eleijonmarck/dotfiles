{
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "trace.server": "verbose",
      "filetypes": ["go"]
    }
  },
  "go.goplsOptions": {
    "completeUnimported": true
  },
  "coc.preferences.formatOnSaveFiletypes": ["css", "markdown", "typescript", "javascript"],
  "eslint.autoFixOnSave": true
}
